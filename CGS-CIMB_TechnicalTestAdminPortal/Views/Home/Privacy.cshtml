@model IEnumerable<CGS_CIMB_TechnicalTestAdminPortal.Models.ReportsDTO>

@{
    ViewData["Title"] = "Home Page";
}

<table id="zero_config" class="table table-striped table-bordered">
    <thead>
        <tr>
            @*<th>
                    @Html.DisplayNameFor(model => model.Id)
                </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReadershipStats)
            </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                @*<td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.Title)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ReadershipStats)
                </td>
                <td>
                    @*<a class="btn btn-cyan btn-sm" asp-action="Edit" asp-route-id="@item.Id">
                            <span class="fa fa-edit"></span> Edit
                        </a>*@
                    <a class="btn btn-success btn-sm" asp-action="Privacy" asp-route-id="@item.Id" target="_blank">
                        <span class="fa fa-eye"></span> View
                    </a>
                    @*<input type="button" onclick="GetRptInfo('@item.Id')" value ="View">*@
                    @*<a class="btn btn-danger btn-sm" asp-action="Delete" asp-route-id="@item.Id">
                            <span class="fa fa-trash-alt"></span> Delete
                        </a>*@
                </td>
            </tr>
        }
    </tbody>
</table>
